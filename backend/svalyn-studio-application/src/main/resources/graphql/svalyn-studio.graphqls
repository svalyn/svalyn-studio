}

type ProfileActivityEntriesConnection {
  edges: [ProfileActivityEntriesEdge!]!
  pageInfo: PageInfo!
}

type ProfileActivityEntriesEdge {
  node: ActivityEntry!
}

type Organization {
  identifier: ID!
  name: String!
