  lastModifiedOn: Instant!
  lastModifiedBy: Profile!
  tags(page: Int!, rowsPerPage: Int!): ProjectTagsConnection!
  activityEntries(page: Int!, rowsPerPage: Int!): ProjectActivityEntriesConnection!
